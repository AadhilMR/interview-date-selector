{
    "functions": {
        "api/*.php": { "runtime": "vercel-php@0.7.3" }
    },
    "routes": [
        { "src": "/(.*)", "dest": "/api/index.php" },
        { "src": "/(.css)", "dest": "/api/style.css" },
        { "src": "/(.css)", "dest": "/api/checkbox.css" },
        { "src": "/(.js)", "dest": "/api/EmailValidator.js" },
        { "src": "/(.js)", "dest": "/api/firebase_config.js" },
        { "src": "/(.js)", "dest": "/api/firebase.js" },
        { "src": "/(.js)", "dest": "/api/request.js" },
        { "src": "/(.js)", "dest": "/api/script.js" }
    ]
}